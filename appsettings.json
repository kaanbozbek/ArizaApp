{
  "ConnectionStrings": {
    "DefaultConnectionString": "#{connectionString}#"
  },
  "DefaultAdminUser":{
    "UserName":"#{username}#",
    "Password":"#{password}#",
    "Email": "#{mail}#",
    "Name":"#{name}#",
    "Surname":"#{surname}#",
    "Department":"#{department}#",
    "Note": "#{note}#"
  },
  "MailOptions": {
    "Host": "#{host}#",
    "Port": "#{port}#",
    "EnableSsl": "#{ssl}#",
    "Sender": "#{sender}#",
    "UserName": "#{mailUsername}#",
    "Password": "#{mailPassword}#"
  },
  "LogPath": "#{logPath}#",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
