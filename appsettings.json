{
  "ConnectionStrings": {
    "DefaultConnectionString": "#{connectionString}#"
  },

  "DefaultAdminUser":{
    "UserName":"#{username}#",
    "Password":"#{password}#",
    "Email": "#{mail}#",
    "Name":"#{name}#",
    "Surname":"#{surname}#",
    "Department":"#{department}#",
    "Note": "#{note}#"
  },
  "LogPath": "#{logPath}#",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
